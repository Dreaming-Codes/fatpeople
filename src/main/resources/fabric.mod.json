	"authors": [
		"Dreaming Codes"
	],
	"contact": {
		"homepage": "https://github.com/Dreaming-Codes/fatpeople",
		"sources": "https://github.com/Dreaming-Codes/fatpeople"
	},
	"license": "MIT",
	"icon": "assets/fat-people/icon.png",
